<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="studyMapper">
     <!-- 별칭을 등록하지 않은 경우 -->
      <select id="selectTest" resultType="com.my.vo.testVO" >
      		SELECT * FROM TEST
      </select>
     <!-- 별칭을 등록한 경우 -->
     <!--<select id="selectTest" resultType="testVO">
        SELECT * FROM TEST
    </select>-->
    <select id="selectOneTest" resultType="com.my.vo.testVO" parameterType="java.lang.String">
      	SELECT * FROM TEST where no=#{no}
    </select>
    <insert id="insertTest" parameterType="com.my.vo.testVO">
    	insert into test(test) values (#{test})
    </insert>
    <update id="updateTest">
    	update test set test=#{test} where no=#{no}
    </update>
    <delete id="deleteTest">
    	delete from test where no=#{no}
    </delete>
</mapper> 