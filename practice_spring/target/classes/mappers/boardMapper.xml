<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="boardMapper">
      <select id="selectTest" resultType="com.my.vo.boardVO" >
      		SELECT * FROM TEST
      </select>
    <select id="selectOneTest" resultType="com.my.vo.boardVO" parameterType="java.lang.String">
      	SELECT * FROM TEST where no=#{no}
    </select>
    <insert id="insertTest" parameterType="com.my.vo.boardVO">
    	insert into test(test,note) values (#{test},#{note})
    </insert>
    <update id="updateTest">
    	update test set test=#{test},note=#{note} where no=#{no}
    </update>
    <delete id="deleteTest">
    	delete from test where no=#{no}
    </delete>
</mapper>